freq <- as.numeric(table(x))
freq_rel <- freq / sum(freq)
-sum(freq_rel * log(freq_rel))
}
Shannon_norm <- function(x) {
freq <- as.numeric(table(x))
freq_rel <- freq / sum(freq)
k <- length(freq)
-sum(freq_rel * log(freq_rel)) / log(k)
}
